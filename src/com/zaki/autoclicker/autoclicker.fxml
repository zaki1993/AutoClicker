<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zaki.autoclicker.Controller">
    <children>
        <AnchorPane id="rootPane" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Label alignment="CENTER" layoutX="155.0" layoutY="177.0" style="&#10;" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
                <TextField fx:id="xPos" id="xPos" layoutX="61.0" layoutY="40.0" />
                <TextField fx:id="yPos" id="yPos" layoutX="246.0" layoutY="40.0" />
                <Label layoutX="39.0" layoutY="44.0" text="X:" />
                <Label layoutX="230.0" layoutY="44.0" text="Y:" />
                <Button fx:id="startBtn" id="startBtn" layoutX="420.0" layoutY="40.0" mnemonicParsing="false" text="Start" onAction="#onStart" />
                <Button fx:id="stopBtn" id="stopBtn" layoutX="480.0" layoutY="40.0" mnemonicParsing="false" text="Stop" onAction="#onStop" />
                <Label layoutX="480.0" layoutY="70.0" text="To stop press: F6" />
                <Label layoutX="14.0" layoutY="78.0" text="Interval:" />
                <TextField fx:id="interval" id="interval" layoutX="61.0" layoutY="74.0" />
            </children>
        </AnchorPane>
    </children>
</VBox>
